<html>
    <head>
        <meta charset="utf-8">
    </head>
    <body>
        <h1> Books list </h1>

        <table>
            <tr>
                <td>ID</td>
                <td>Title</td>
            </tr> 
            <for|book| of=data.books>     
               <tr>
                <td>${book.id}</td>
                <td>${book.title}</td>
              </tr>     
            </for>
        </table> 
    </body> 
</html>